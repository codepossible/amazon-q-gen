{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Next.js Debug",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/postgres-table-app/node_modules/.bin/next",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Client Debugging",
            "skipFiles": [
                "/**"
            ],
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/postgres-table-app"
        }
    ]
}